<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestCompanyRoleRequest">
        <property name="companyUuid" type="string" />
        <property name="idCustomer" type="int" />
        <property name="idCompany" type="int" />
    </transfer>

    <transfer name="RestCompanyRoleResponse">
        <property name="companyRoleCollection" type="CompanyRoleCollection" />
        <property name="isSuccess" type="bool" />
    </transfer>

    <transfer name="RestCompanyRoleAttributes">
        <property name="name" type="string" />
        <property name="isDefault" type="bool" />
    </transfer>
</transfers>
